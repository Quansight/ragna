{
  "openapi": "3.1.0",
  "info": { "title": "FastAPI", "version": "0.1.0" },
  "paths": {
    "/health": {
      "get": {
        "summary": "Health",
        "operationId": "health_health_get",
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": {
              "application/json": {
                "schema": {
                  "type": "string",
                  "title": "Response Health Health Get"
                }
              }
            }
          }
        }
      }
    },
    "/components": {
      "get": {
        "summary": "Get Components",
        "operationId": "get_components_components_get",
        "parameters": [
          {
            "required": true,
            "schema": { "type": "string", "title": "User" },
            "name": "user",
            "in": "query"
          }
        ],
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": {
              "application/json": {
                "schema": { "$ref": "#/components/schemas/Components" }
              }
            }
          },
          "422": {
            "description": "Validation Error",
            "content": {
              "application/json": {
                "schema": { "$ref": "#/components/schemas/HTTPValidationError" }
              }
            }
          }
        }
      }
    },
    "/document": {
      "get": {
        "summary": "Get Document Upload Info",
        "operationId": "get_document_upload_info_document_get",
        "parameters": [
          {
            "required": true,
            "schema": { "type": "string", "title": "Name" },
            "name": "name",
            "in": "query"
          },
          {
            "required": true,
            "schema": { "type": "string", "title": "User" },
            "name": "user",
            "in": "query"
          }
        ],
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": {
              "application/json": {
                "schema": { "$ref": "#/components/schemas/DocumentUploadInfo" }
              }
            }
          },
          "422": {
            "description": "Validation Error",
            "content": {
              "application/json": {
                "schema": { "$ref": "#/components/schemas/HTTPValidationError" }
              }
            }
          }
        }
      },
      "post": {
        "summary": "Upload Document",
        "operationId": "upload_document_document_post",
        "requestBody": {
          "content": {
            "multipart/form-data": {
              "schema": {
                "$ref": "#/components/schemas/Body_upload_document_document_post"
              }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": {
              "application/json": {
                "schema": { "$ref": "#/components/schemas/Document" }
              }
            }
          },
          "422": {
            "description": "Validation Error",
            "content": {
              "application/json": {
                "schema": { "$ref": "#/components/schemas/HTTPValidationError" }
              }
            }
          }
        }
      }
    },
    "/chats": {
      "get": {
        "summary": "Get Chats",
        "operationId": "get_chats_chats_get",
        "parameters": [
          {
            "required": true,
            "schema": { "type": "string", "title": "User" },
            "name": "user",
            "in": "query"
          }
        ],
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": {
              "application/json": {
                "schema": {
                  "items": { "$ref": "#/components/schemas/Chat" },
                  "type": "array",
                  "title": "Response Get Chats Chats Get"
                }
              }
            }
          },
          "422": {
            "description": "Validation Error",
            "content": {
              "application/json": {
                "schema": { "$ref": "#/components/schemas/HTTPValidationError" }
              }
            }
          }
        }
      },
      "post": {
        "summary": "Create Chat",
        "operationId": "create_chat_chats_post",
        "parameters": [
          {
            "required": true,
            "schema": { "type": "string", "title": "User" },
            "name": "user",
            "in": "query"
          }
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": { "$ref": "#/components/schemas/ChatMetadataCreate" }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": {
              "application/json": {
                "schema": { "$ref": "#/components/schemas/Chat" }
              }
            }
          },
          "422": {
            "description": "Validation Error",
            "content": {
              "application/json": {
                "schema": { "$ref": "#/components/schemas/HTTPValidationError" }
              }
            }
          }
        }
      }
    },
    "/chats/{id}": {
      "get": {
        "summary": "Get Chat",
        "operationId": "get_chat_chats__id__get",
        "parameters": [
          {
            "required": true,
            "schema": { "type": "string", "format": "uuid", "title": "Id" },
            "name": "id",
            "in": "path"
          },
          {
            "required": true,
            "schema": { "type": "string", "title": "User" },
            "name": "user",
            "in": "query"
          }
        ],
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": {
              "application/json": {
                "schema": { "$ref": "#/components/schemas/Chat" }
              }
            }
          },
          "422": {
            "description": "Validation Error",
            "content": {
              "application/json": {
                "schema": { "$ref": "#/components/schemas/HTTPValidationError" }
              }
            }
          }
        }
      }
    },
    "/chats/{id}/start": {
      "post": {
        "summary": "Start Chat",
        "operationId": "start_chat_chats__id__start_post",
        "parameters": [
          {
            "required": true,
            "schema": { "type": "string", "format": "uuid", "title": "Id" },
            "name": "id",
            "in": "path"
          },
          {
            "required": true,
            "schema": { "type": "string", "title": "User" },
            "name": "user",
            "in": "query"
          }
        ],
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": {
              "application/json": {
                "schema": { "$ref": "#/components/schemas/Chat" }
              }
            }
          },
          "422": {
            "description": "Validation Error",
            "content": {
              "application/json": {
                "schema": { "$ref": "#/components/schemas/HTTPValidationError" }
              }
            }
          }
        }
      }
    },
    "/chats/{id}/close": {
      "post": {
        "summary": "Close Chat",
        "operationId": "close_chat_chats__id__close_post",
        "parameters": [
          {
            "required": true,
            "schema": { "type": "string", "format": "uuid", "title": "Id" },
            "name": "id",
            "in": "path"
          },
          {
            "required": true,
            "schema": { "type": "string", "title": "User" },
            "name": "user",
            "in": "query"
          }
        ],
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": {
              "application/json": {
                "schema": { "$ref": "#/components/schemas/Chat" }
              }
            }
          },
          "422": {
            "description": "Validation Error",
            "content": {
              "application/json": {
                "schema": { "$ref": "#/components/schemas/HTTPValidationError" }
              }
            }
          }
        }
      }
    },
    "/chats/{id}/answer": {
      "post": {
        "summary": "Answer",
        "operationId": "answer_chats__id__answer_post",
        "parameters": [
          {
            "required": true,
            "schema": { "type": "string", "format": "uuid", "title": "Id" },
            "name": "id",
            "in": "path"
          },
          {
            "required": true,
            "schema": { "type": "string", "title": "Prompt" },
            "name": "prompt",
            "in": "query"
          },
          {
            "required": true,
            "schema": { "type": "string", "title": "User" },
            "name": "user",
            "in": "query"
          }
        ],
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": {
              "application/json": {
                "schema": { "$ref": "#/components/schemas/AnswerOutput" }
              }
            }
          },
          "422": {
            "description": "Validation Error",
            "content": {
              "application/json": {
                "schema": { "$ref": "#/components/schemas/HTTPValidationError" }
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "AnswerOutput": {
        "properties": {
          "message": { "$ref": "#/components/schemas/Message" },
          "chat": { "$ref": "#/components/schemas/Chat" }
        },
        "type": "object",
        "required": ["message", "chat"],
        "title": "AnswerOutput"
      },
      "Body_upload_document_document_post": {
        "properties": {
          "token": { "type": "string", "title": "Token" },
          "file": { "type": "string", "format": "binary", "title": "File" }
        },
        "type": "object",
        "required": ["token", "file"],
        "title": "Body_upload_document_document_post"
      },
      "Chat": {
        "properties": {
          "id": { "type": "string", "format": "uuid", "title": "Id" },
          "metadata": { "$ref": "#/components/schemas/ChatMetadata" },
          "messages": {
            "items": { "$ref": "#/components/schemas/Message" },
            "type": "array",
            "title": "Messages"
          },
          "started": { "type": "boolean", "title": "Started" },
          "closed": { "type": "boolean", "title": "Closed" }
        },
        "type": "object",
        "required": ["id", "metadata", "messages", "started", "closed"],
        "title": "Chat"
      },
      "ChatMetadata": {
        "properties": {
          "name": { "type": "string", "title": "Name" },
          "source_storage": { "type": "string", "title": "Source Storage" },
          "assistant": { "type": "string", "title": "Assistant" },
          "params": { "type": "object", "title": "Params" },
          "documents": {
            "items": { "$ref": "#/components/schemas/Document" },
            "type": "array",
            "title": "Documents"
          }
        },
        "type": "object",
        "required": [
          "name",
          "source_storage",
          "assistant",
          "params",
          "documents"
        ],
        "title": "ChatMetadata"
      },
      "ChatMetadataCreate": {
        "properties": {
          "name": { "type": "string", "title": "Name" },
          "source_storage": { "type": "string", "title": "Source Storage" },
          "assistant": { "type": "string", "title": "Assistant" },
          "params": { "type": "object", "title": "Params" },
          "document_ids": {
            "items": { "type": "string", "format": "uuid" },
            "type": "array",
            "title": "Document Ids"
          }
        },
        "type": "object",
        "required": [
          "name",
          "source_storage",
          "assistant",
          "params",
          "document_ids"
        ],
        "title": "ChatMetadataCreate"
      },
      "Components": {
        "properties": {
          "source_storages": {
            "items": { "type": "string" },
            "type": "array",
            "title": "Source Storages"
          },
          "assistants": {
            "items": { "type": "string" },
            "type": "array",
            "title": "Assistants"
          }
        },
        "type": "object",
        "required": ["source_storages", "assistants"],
        "title": "Components"
      },
      "Document": {
        "properties": {
          "id": { "type": "string", "format": "uuid", "title": "Id" },
          "name": { "type": "string", "title": "Name" }
        },
        "type": "object",
        "required": ["id", "name"],
        "title": "Document"
      },
      "DocumentUploadInfo": {
        "properties": {
          "url": {
            "type": "string",
            "maxLength": 2083,
            "minLength": 1,
            "format": "uri",
            "title": "Url"
          },
          "data": { "type": "object", "title": "Data" },
          "document": { "$ref": "#/components/schemas/Document" }
        },
        "type": "object",
        "required": ["url", "data", "document"],
        "title": "DocumentUploadInfo"
      },
      "HTTPValidationError": {
        "properties": {
          "detail": {
            "items": { "$ref": "#/components/schemas/ValidationError" },
            "type": "array",
            "title": "Detail"
          }
        },
        "type": "object",
        "title": "HTTPValidationError"
      },
      "Message": {
        "properties": {
          "id": { "type": "string", "format": "uuid", "title": "Id" },
          "role": { "$ref": "#/components/schemas/MessageRole" },
          "content": { "type": "string", "title": "Content" },
          "sources": {
            "items": { "$ref": "#/components/schemas/Source" },
            "type": "array",
            "title": "Sources"
          },
          "timestamp": {
            "type": "string",
            "format": "date-time",
            "title": "Timestamp"
          }
        },
        "type": "object",
        "required": ["id", "role", "content", "sources", "timestamp"],
        "title": "Message"
      },
      "MessageRole": {
        "enum": ["system", "user", "assistant"],
        "title": "MessageRole",
        "description": "An enumeration."
      },
      "Source": {
        "properties": {
          "id": { "type": "string", "format": "uuid", "title": "Id" },
          "document": { "$ref": "#/components/schemas/Document" },
          "location": { "type": "string", "title": "Location" }
        },
        "type": "object",
        "required": ["id", "document", "location"],
        "title": "Source"
      },
      "ValidationError": {
        "properties": {
          "loc": {
            "items": { "anyOf": [{ "type": "string" }, { "type": "integer" }] },
            "type": "array",
            "title": "Location"
          },
          "msg": { "type": "string", "title": "Message" },
          "type": { "type": "string", "title": "Error Type" }
        },
        "type": "object",
        "required": ["loc", "msg", "type"],
        "title": "ValidationError"
      }
    }
  }
}
